  "license": "MIT",
  "engines": {
    "node": "6.11.0"
  },
  "dependencies": {
    "concurrently": "^3.4.0",
    "dotenv-webpack": "^1.4.5",
    "lodash": "^4.17.4"
  },
  "scripts": {
    "build": "concurrently \"cd client && yarn build\" \"cd server && yarn build\"",
    "install": "(cd client && yarn) && (cd server && yarn)",
